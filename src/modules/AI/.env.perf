# Performance Optimization - Timeout Configuration

# Reduce all client timeouts for faster failure detection
# Old values were 15-30s, causing long hangs

# NestJS Backend (chat persistence)
NEST_CLIENT_TIMEOUT=5.0

# Service Clients (reduce from 15s to 5s)
BOOKING_CLIENT_TIMEOUT=5.0
CARRIER_CLIENT_TIMEOUT=5.0
SLOT_CLIENT_TIMEOUT=5.0
ANALYTICS_CLIENT_TIMEOUT=5.0
BLOCKCHAIN_CLIENT_TIMEOUT=5.0
BOOKING_WRITE_CLIENT_TIMEOUT=5.0

# AGNO Configuration
AGNO_ENABLED=false
AGNO_TIMEOUT=6.0
# OPENAI_API_KEY=your-key-here

# Orchestrator Configuration
ORCHESTRATOR_DEADLINE=8.0  # Maximum time for entire orchestration
ORCHESTRATOR_AGENT_TIMEOUT=4.0  # Maximum time per agent execution
